import random

# Создание класса "Маг"
class Mage:
    def __init__(self, name, power):
        self.name = name
        self.power = power
    
    def cast_spell(self):
        spells = ["Fireball", "Frostbolt", "Arcane Missiles", "Summon Elemental"]
        spell = random.choice(spells)
        print(f"{self.name} casts {spell} with {self.power} power!")

# Создание объектов класса "Маг"
mage1 = Mage("Gandalf", 100)
mage2 = Mage("Medivh", 80)
mage3 = Mage("Merlin", 120)

# Вызов метода "cast_spell" для каждого мага
mage1.cast_spell()
mage2.cast_spell()
mage3.cast_spell()
